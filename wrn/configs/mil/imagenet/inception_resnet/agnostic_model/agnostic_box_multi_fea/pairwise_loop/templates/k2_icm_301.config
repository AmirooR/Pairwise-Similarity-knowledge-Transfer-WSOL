model {
  wrn_attention {
    k_shot: 2
    num_negative_bags: 0
    bag_size: 301
    use_features: true
    num_classes: 100
    wrn_depth: 28
    wrn_width: 10
    wrn_dropout_rate: 0
    wrn_data_format: "channels_last"
    weight_decay: 0.0005
    attention_tree {
      training_subsampler {
        ncobj_proposals: 10
        positive_balance_fraction: 0.25
      }
      fea_split_ind: 4096
      preprocess_convline {
        conv_hyperparameters {
          op: CONV
          batch_norm {
            scale: true
            epsilon: 1e-5
          }
          regularizer {
            l2_regularizer {
              weight: 0.0005
            }
          }
          initializer {
            variance_scaling_initializer {
              factor: 1.0
              uniform: true
              mode: FAN_AVG
            }
          }
        }
        filter_num_list: 640
      }
      #k1
      unit {
        cross_similarity {
          deep_cross_similarity {
            fc_hyperparameters {
              op: FC
              regularizer {
                l2_regularizer {
                  weight: 0.0005
                }
              }
              initializer {
                variance_scaling_initializer {
                  factor: 1.0
                  uniform: true
                  mode: FAN_AVG
                }
              }
            }
          }
        }
        loss {
          sigmoid_cross_entropy_loss {}
          class_agnostic: false
          weight: 0.0
        }
        training_subsampler {
          ncobj_proposals: 10
        }
        ncobj_proposals: 301
        orig_fea_in_post_convline: true
        use_tanh_sigmoid_in_post_convline: false
        post_convline_res: "none"
        overwrite_fea_by_scores: false
        compute_scores_after_matching: true
        transfered_objectness_weight: 1.0
      }
      #k2
      unit {
        cross_similarity {
          double_cross_similarity {
            fea_split_ind: 640
            main_weight: 0.75
            main {
              pairwise_cross_similarity {
                cross_similarity {
                  deep_cross_similarity {
                    fc_hyperparameters {
                      op: FC
                      regularizer {
                        l2_regularizer {
                          weight: 0.0005
                        }
                      }
                      initializer {
                        variance_scaling_initializer {
                          factor: 1.0
                          uniform: true
                          mode: FAN_AVG
                        }
                      }
                    }
                    stop_gradient: false
                  }
                }
              }
            }
          }
        } 
        loss {
          sigmoid_cross_entropy_loss {}
          class_agnostic: false
          weight: 1.0
        }
        post_convline {
          conv_hyperparameters {
            op: CONV
            batch_norm {
              scale: true
              epsilon: 1e-5
            }
            regularizer {
              l2_regularizer {
                weight: 5e-4
              }
            }
            initializer {
              variance_scaling_initializer {
                factor: 1.0
                uniform: true
                mode: FAN_AVG
              }
            }
          }
          filter_num_list: 640
        }
        training_subsampler {
          stratified: true
          ncobj_proposals: 6
        }
        ncobj_proposals: 500
        orig_fea_in_post_convline: true
        use_tanh_sigmoid_in_post_convline: true
        post_convline_res: "sum"
        overwrite_fea_by_scores: false
        compute_scores_after_matching: true
        unary_energy_scale: 0.0 #0.06
      }
    }
  }
}

train_config: {
  batch_size: 4
  optimizer {
    momentum_optimizer: {
      learning_rate: {
        manual_step_learning_rate {
          initial_learning_rate: 0.001
          schedule {
            step: 0
            learning_rate: .001
          }
          schedule {
            step: 70000
            learning_rate: .0001
          }
          schedule {
            step: 200000
            learning_rate: .000016
          }
        }
      }
      momentum_optimizer_value: 0.9
    }
    use_moving_average: false
  }
  from_detection_checkpoint: true
  gradient_clipping_by_norm: 10.0
  num_steps: 210000
}

train_input_reader {
  mil_det_fea_input_reader{
    shuffle: true
    num_readers: 2
    split: "trws_k32_aggregate_init"
    support_db: "../feature_extractor/logs/imagenet/revisit_agnostic_box_multi_feas"
    support_db_name: "trws_k32_aggregate_init"
    image_size: [512,512]
    fea_dim: 5632 #4096 + 1536
    feas_key: "feas"
    feas_key: "det_feas"
  }
}

eval_config: {
  num_examples: 300
  num_visualizations: 0
  eval_interval_secs: 1000
  num_top_predictions: 1
  use_target_class_in_predictions: true
  #metrics_set: "pascal_voc_metrics"
  load_from_vanilla_into_doubled_net: true
  transfered_network_checkpoint: "./logs/mil/imagenet/inception_resnet/agnostic_model/agnostic_box_multi_fea/k2n0/train/top/model.ckpt-71860"
  aggregation_params:
  {
    aggregate: false
    dataset_root: "../feature_extractor/logs/imagenet/revisit_agnostic_box_multi_feas"
    groundtruth_split: "alexfea300_revisit_val1"
    save_split: "temp" 
    save_eval_freq: 150
    update_split: "temp"
  }
}

eval_input_reader: {
  mil_det_fea_input_reader{
    shuffle: false
    num_readers: 1
    label_map_path: ""
    split: "alexfea300_revisit_val1"
    support_db: "../feature_extractor/logs/imagenet/revisit_agnostic_box_multi_feas"
    support_db_name: "alexfea300_revisit_val1"
    image_size: [512,512]
    fea_dim: 5632 #4096 + 1536
    feas_key: "feas"
    feas_key: "det_feas"
    objectness_key: "objectness"
  }
}

calib_config: {
  batch_size: 2
  optimizer {
    momentum_optimizer: {
      learning_rate: {
        manual_step_learning_rate {
          initial_learning_rate: 0.00001
          schedule {
            step: 0
            learning_rate: .00001
          }
          schedule {
            step: 60000
            learning_rate: .000001
          }
        }
      }
      momentum_optimizer_value: 0.9
    }
    use_moving_average: false
  }
  from_detection_checkpoint: true
  fine_tune_checkpoint: "./logs/pcvpr2019/k16_pw/train/model.ckpt-200000"
  num_steps: 30000
}

calib_input_reader {
  mil_input_reader{
    shuffle: true
    num_readers: 4
    data_format: "channels_last"
    has_bag_iterator: true
    do_pp_mean: false
    dataset_name: "miniimagenet"
    split: "train"
    train_num: 600
    num_sample_classes: 15
  }
}
