model {
  wrn_attention {
    k_shot: 8
    num_negative_bags: 8 
    bag_size: 300
    use_features: true
    num_classes: 148
    wrn_depth: 28
    wrn_width: 10
    wrn_dropout_rate: 0
    wrn_data_format: "channels_last"
    weight_decay: 0.0005
    attention_tree {
      #k1
      unit {
        cross_similarity {
          k1_cross_similarity {
            topk: 50
            cross_similarity {
              deep_cross_similarity {
                fc_hyperparameters {
                  op: FC
                  regularizer {
                    l2_regularizer {
                      weight: 0.0005
                    }
                  }
                  initializer {
                    variance_scaling_initializer {
                      factor: 1.0
                      uniform: true
                      mode: FAN_AVG
                    }
                  }
                }
                stop_gradient: false
              }
            }
            mode: "SOFTMAX"
            share_weights_with_pairwise_cs: false
          }
        }
        post_convline {
          conv_hyperparameters {
            op: CONV
            batch_norm {
              scale: true
              epsilon: 1e-5
            }
            regularizer {
              l2_regularizer {
                weight: 5e-4
              }
            }
            initializer {
              variance_scaling_initializer {
                factor: 1.0
                uniform: true
                mode: FAN_AVG
              }
            }
          }
          filter_num_list: 640
        }
        loss {
          sigmoid_cross_entropy_loss {}
          class_agnostic: true
          weight: 1.0
        }
        training_subsampler {
          ncobj_proposals: 5
          topk: 5
        } 
        ncobj_proposals: 300 
        orig_fea_in_post_convline: true
        use_tanh_sigmoid_in_post_convline: true
        post_convline_res: "sum"
        overwrite_fea_by_scores: false
        compute_scores_after_matching: true
      }
      #k2
      unit {
        cross_similarity {
          pairwise_cross_similarity {
            cross_similarity {
              deep_cross_similarity {
                fc_hyperparameters {
                  op: FC
                  regularizer {
                    l2_regularizer {
                      weight: 0.0005
                    }
                  }
                  initializer {
                    variance_scaling_initializer {
                      factor: 1.0
                      uniform: true
                      mode: FAN_AVG
                    }
                  }
                }
              }
            } 
          }
        }
        loss {
          sigmoid_cross_entropy_loss {}
          class_agnostic: true
          weight: 1.0
        }
        post_convline {
          conv_hyperparameters {
            op: CONV
            batch_norm {
              scale: true
              epsilon: 1e-5
            }
            regularizer {
              l2_regularizer {
                weight: 5e-4
              }
            }
            initializer {
              variance_scaling_initializer {
                factor: 1.0
                uniform: true
                mode: FAN_AVG
              }
            }
          }
          filter_num_list: 640
        } 
        training_subsampler {
          ncobj_proposals: 6
        }
        ncobj_proposals: 300
        orig_fea_in_post_convline: true
        use_tanh_sigmoid_in_post_convline: true
        post_convline_res: "sum"
        overwrite_fea_by_scores: false
        compute_scores_after_matching: true
        unary_energy_scale: 0.7 #1.4
      }
      #k4
      unit {
        cross_similarity {
          pairwise_cross_similarity {
            cross_similarity {
              deep_cross_similarity {
                fc_hyperparameters {
                  op: FC
                  regularizer {
                    l2_regularizer {
                      weight: 0.0005
                    }
                  }
                  initializer {
                    variance_scaling_initializer {
                      factor: 1.0
                      uniform: true
                      mode: FAN_AVG
                    }
                  }
                }
              }
            } 
          }
        }
        loss {
          sigmoid_cross_entropy_loss {}
          class_agnostic: true
          weight: 1.0
        }
        post_convline {
          conv_hyperparameters {
            op: CONV
            batch_norm {
              scale: true
              epsilon: 1e-5
            }
            regularizer {
              l2_regularizer {
                weight: 5e-4
              }
            }
            initializer {
              variance_scaling_initializer {
                factor: 1.0
                uniform: true
                mode: FAN_AVG
              }
            }
          }
          filter_num_list: 640
        } 
        training_subsampler {
          ncobj_proposals: 6
        }
        ncobj_proposals: 300
        orig_fea_in_post_convline: true
        use_tanh_sigmoid_in_post_convline: true
        post_convline_res: "sum"
        overwrite_fea_by_scores: false
        compute_scores_after_matching: true
        unary_energy_scale: 0.7 #0.9
      }
      #k8
      unit {
        cross_similarity {
          pairwise_cross_similarity {
            cross_similarity {
              deep_cross_similarity {
                fc_hyperparameters {
                  op: FC
                  regularizer {
                    l2_regularizer {
                      weight: 0.0005
                    }
                  }
                  initializer {
                    variance_scaling_initializer {
                      factor: 1.0
                      uniform: true
                      mode: FAN_AVG
                    }
                  }
                }
              }
            } 
          }
        }
        loss {
          sigmoid_cross_entropy_loss {}
          class_agnostic: true
          weight: 1.0
        }
        post_convline {
          conv_hyperparameters {
            op: CONV
            batch_norm {
              scale: true
              epsilon: 1e-5
            }
            regularizer {
              l2_regularizer {
                weight: 5e-4
              }
            }
            initializer {
              variance_scaling_initializer {
                factor: 1.0
                uniform: true
                mode: FAN_AVG
              }
            }
          }
          filter_num_list: 640
        } 
        training_subsampler {
          ncobj_proposals: 6
        }
        ncobj_proposals: 1
        orig_fea_in_post_convline: true
        use_tanh_sigmoid_in_post_convline: true
        post_convline_res: "sum"
        overwrite_fea_by_scores: false
        compute_scores_after_matching: true
        unary_energy_scale: 0.7
      }
    }
  }
}

train_config: {
  batch_size: 8
  optimizer {
    momentum_optimizer: {
      learning_rate: {
        manual_step_learning_rate {
          initial_learning_rate: 0.0
          schedule {
            step: 0
            learning_rate: .0
          }
        }
      }
      momentum_optimizer_value: 0.9
    }
    use_moving_average: false
  }
  from_detection_checkpoint: false
  num_steps: 1
}

train_input_reader {
  mil_det_fea_input_reader{
    shuffle: true
    num_readers: 2
    split: "train"
    support_db: "../feature_extractor/logs/imagenet/k1/extract300"
    support_db_name: "train"
  }
}

eval_config: {
  num_examples: 1000
  num_visualizations: 0
  eval_interval_secs: 1000
  num_top_predictions: 1
}

eval_input_reader: {
  mil_det_fea_input_reader{
    shuffle: false
    num_readers: 1
    label_map_path: ""
    split: "iccv"
    support_db: "../feature_extractor/logs/imagenet/k1_iccv/extract300"
    support_db_name: "iccv"
  }
}

calib_config: {
  batch_size: 2
  optimizer {
    momentum_optimizer: {
      learning_rate: {
        manual_step_learning_rate {
          initial_learning_rate: 0.00001
          schedule {
            step: 0
            learning_rate: .00001
          }
          schedule {
            step: 60000
            learning_rate: .000001
          }
        }
      }
      momentum_optimizer_value: 0.9
    }
    use_moving_average: false
  }
  num_steps: 30000
}

calib_input_reader {
  mil_det_fea_input_reader{
    shuffle: true
    num_readers: 2
    split: "train"
    support_db: "../feature_extractor/logs/imagenet/k1/extract300"
    support_db_name: "train"
  }
}
