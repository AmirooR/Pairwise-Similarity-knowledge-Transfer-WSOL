model {
  wrn_attention {
    k_shot: 2
    num_negative_bags: 2
    bag_size: 5
    use_features: true
    num_classes: 64
    wrn_depth: 28
    wrn_width: 10
    wrn_dropout_rate: 0
    wrn_data_format: "channels_last"
    weight_decay: 0.0005
    attention_tree {
      #k1
      unit {
        cross_similarity {
          k1_cross_similarity {
            cross_similarity {
              deep_cross_similarity {
                fc_hyperparameters {
                  op: FC
                  regularizer {
                    l2_regularizer {
                      weight: 0.0005
                    }
                  }
                  initializer {
                    variance_scaling_initializer {
                      factor: 1.0
                      uniform: true
                      mode: FAN_AVG
                    }
                  }
                }
                stop_gradient: false
              }
            }
            mode: "MEAN"
            share_weights_with_pairwise_cs: false
          }
        }
        post_convline {
          conv_hyperparameters {
            op: CONV
            batch_norm {
              scale: true
              epsilon: 1e-5
            }
            regularizer {
              l2_regularizer {
                weight: 5e-4
              }
            }
            initializer {
              variance_scaling_initializer {
                factor: 1.0
                uniform: true
                mode: FAN_AVG
              }
            }
          }
          filter_num_list: 640
        } 
        loss {
          sigmoid_cross_entropy_loss {}
          class_agnostic: true
          weight: 1.0
        }
        training_subsampler {
          ncobj_proposals: 5
          topk: 5
        } 
        ncobj_proposals: 5 
        orig_fea_in_post_convline: true
        use_tanh_sigmoid_in_post_convline: true
        post_convline_res: "sum"
        overwrite_fea_by_scores: false
        compute_scores_after_matching: true
      }
      #k2
      unit {
        cross_similarity {
          deep_cross_similarity {
            fc_hyperparameters {
              op: FC
              regularizer {
                l2_regularizer {
                  weight: 0.0005
                }
              }
              initializer {
                variance_scaling_initializer {
                  factor: 1.0
                  uniform: true
                  mode: FAN_AVG
                }
              }
            }
            stop_gradient: false
          }
        } 
        loss {
          pairwise_estimate_loss {}
          class_agnostic: true
          weight: 1.0
        }
        post_convline {
          conv_hyperparameters {
            op: CONV
            batch_norm {
              scale: true
              epsilon: 1e-5
            }
            regularizer {
              l2_regularizer {
                weight: 5e-4
              }
            }
            initializer {
              variance_scaling_initializer {
                factor: 1.0
                uniform: true
                mode: FAN_AVG
              }
            }
          }
          filter_num_list: 640
        }
        training_subsampler {
          ncobj_proposals: 25
        } 
        ncobj_proposals: 25
        orig_fea_in_post_convline: true
        use_tanh_sigmoid_in_post_convline: true
        post_convline_res: "sum"
        overwrite_fea_by_scores: false
        compute_scores_after_matching: true
      }
    }
  }
}

train_config: {
  batch_size: 8
  optimizer {
    momentum_optimizer: {
      learning_rate: {
        manual_step_learning_rate {
          initial_learning_rate: 0.008
          schedule {
            step: 0
            learning_rate: .008
          }
          schedule {
            step: 10000 
            learning_rate: .0008
          }
          schedule {
            step: 20000
            learning_rate: .00016
          }
          schedule {
            step: 30000
            learning_rate: .0001
          }
          schedule {
            step: 60000
            learning_rate: .00001
          }
        }
      }
      momentum_optimizer_value: 0.9
    }
    use_moving_average: false
  }
  from_detection_checkpoint: false
  num_steps: 80000
}

train_input_reader {
  mil_input_reader{
    shuffle: true
    num_readers: 1
    data_format: "channels_last"
    has_bag_iterator: true
    do_pp_mean: false
    dataset_name: "miniimagenet"
    split: "train"
    train_num: 500
    num_sample_classes: 15
  }
}

eval_config: {
  num_examples: 800
  num_visualizations: 5
  eval_interval_secs: 1000
  num_top_predictions: 1
}

eval_input_reader: {
  mil_input_reader{
    shuffle: false
    num_readers: 1
    data_format: "channels_last"
    has_bag_iterator: true
    do_pp_mean: false
    label_map_path: ""
    is_training: false
    dataset_name: "miniimagenet"
    split: "val"
    train_num: 0
    num_sample_classes: 15
  }
}

eval_input_reader: {
  mil_input_reader{
    shuffle: false
    num_readers: 1
    data_format: "channels_last"
    has_bag_iterator: true
    do_pp_mean: false
    label_map_path: ""
    is_training: false
    dataset_name: "miniimagenet"
    split: "test"
    train_num: 0
    num_sample_classes: 15
  }
}

